<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/KS%20AUTOSAR%20DIPLOMA%20DATA/AUTOSAR%20shema/AUTOSAR_4-2-2.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
		
			<SHORT-NAME>MY_DATA_TYPES</SHORT-NAME>
			<ELEMENTS>
			
				<DATA-CONSTR>
					<SHORT-NAME>DATA_CONSTRAINS</SHORT-NAME>
					<DATA-CONSTR-RULES>
						<DATA-CONSTR-RULE>
							<PHYS-CONSTRS>
								<LOWER-LIMIT>0</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</PHYS-CONSTRS>
							<INTERNAL-CONSTRS>
								<LOWER-LIMIT>0</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</INTERNAL-CONSTRS>
						</DATA-CONSTR-RULE>
					</DATA-CONSTR-RULES>
				</DATA-CONSTR>
				
				<SW-BASE-TYPE>
					<SHORT-NAME>DATA_uint8</SHORT-NAME>
					<CATEGORY>FIXED_LENGTH</CATEGORY>
					<BASE-TYPE-SIZE>8</BASE-TYPE-SIZE>
					<BASE-TYPE-ENCODING>NONE</BASE-TYPE-ENCODING>
					<MEM-ALIGNMENT>1</MEM-ALIGNMENT>
					<NATIVE-DECLARATION>unigned char</NATIVE-DECLARATION>
				</SW-BASE-TYPE>
				
				<IMPLEMENTATION-DATA-TYPE>
					<SHORT-NAME>myuint8</SHORT-NAME>
					<CATEGORY>VALUE</CATEGORY>
					<SW-DATA-DEF-PROPS>
						<SW-DATA-DEF-PROPS-VARIANTS>
							<SW-DATA-DEF-PROPS-CONDITIONAL>
								<BASE-TYPE-REF DEST="SW-BASE-TYPE">/MY_DATA_TYPES/DATA_uint8</BASE-TYPE-REF>
								<DATA-CONSTR-REF DEST="DATA-CONSTR">/MY_DATA_TYPES/DATA_CONSTRAINS</DATA-CONSTR-REF>
							</SW-DATA-DEF-PROPS-CONDITIONAL>
						</SW-DATA-DEF-PROPS-VARIANTS>
					</SW-DATA-DEF-PROPS>
				</IMPLEMENTATION-DATA-TYPE>
				
			</ELEMENTS>
			
		</AR-PACKAGE>
		<AR-PACKAGE>
		
			<SHORT-NAME>INTERFACES</SHORT-NAME>
			<ELEMENTS>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_DOOR_STATE</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>DOOR_STATE</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MY_DATA_TYPES/myuint8</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				
				<CLIENT-SERVER-INTERFACE>
					<SHORT-NAME>CSI_DOOR_SENSOR</SHORT-NAME>
					<OPERATIONS>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>DOOR_SENSOR</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>LED_STATE</SHORT-NAME>
									<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MY_DATA_TYPES/myuint8</TYPE-TREF>
									<DIRECTION>IN</DIRECTION>
								</ARGUMENT-DATA-PROTOTYPE>
							</ARGUMENTS>
						</CLIENT-SERVER-OPERATION>
					</OPERATIONS>
					
					<POSSIBLE-ERRORS>
						<APPLICATION-ERROR>
							<SHORT-NAME>E_OK</SHORT-NAME>
							<ERROR-CODE>0</ERROR-CODE>
						</APPLICATION-ERROR>
						<APPLICATION-ERROR>
							<SHORT-NAME>E_NOT_OK</SHORT-NAME>
							<ERROR-CODE>1</ERROR-CODE>
						</APPLICATION-ERROR>
					</POSSIBLE-ERRORS>
				</CLIENT-SERVER-INTERFACE>
	
			</ELEMENTS>
		</AR-PACKAGE>
		
		
		<AR-PACKAGE>
			<SHORT-NAME>DOOR_INDICATOR_SWCOMPONENTS</SHORT-NAME>
			<ELEMENTS>
			
				<ECU-ABSTRACTION-SW-COMPONENT-TYPE>
					<SHORT-NAME>DOOR_ECU_SWC</SHORT-NAME> 
					<PORTS>
					<P-PORT-PROTOTYPE>
						<SHORT-NAME>PP_SR_DOOR_SATE</SHORT-NAME>
						<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/INTERFACES/SRI_DOOR_STATE/DOOR_STATE</PROVIDED-INTERFACE-TREF>
					</P-PORT-PROTOTYPE>
					<P-PORT-PROTOTYPE>
						<SHORT-NAME>PP_CS_LED_SWITCH</SHORT-NAME>
						<PROVIDED-INTERFACE-TREF DEST="CLIENT-SERVER-INTERFACE">/INTERFACES/CSI_DOOR_SENSOR/DOOR_SENSOR</PROVIDED-INTERFACE-TREF>
					</P-PORT-PROTOTYPE>
					</PORTS>
			
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>ECU_SWC_RUNNABLES</SHORT-NAME>
								<EVENTS>
									<TIMING-EVENT>
										<SHORT-NAME>TE_10ms</SHORT-NAME>
										<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/ECU_SWC_RUNNABLES/DOOR_READ_OP</START-ON-EVENT-REF>
										<PERIOD>0.01</PERIOD>
									</TIMING-EVENT>
									<OPERATION-INVOKED-EVENT>
										<SHORT-NAME>OIV_LED_SWITCH</SHORT-NAME>
										<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/ECU_SWC_RUNNABLES/LED_SWITCH_OP</START-ON-EVENT-REF>
										<OPERATION-IREF>
											<CONTEXT-P-PORT-REF DEST="P-PORT-PROTOTYPE">DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/PP_CS_LED_SWITCH</CONTEXT-P-PORT-REF>
											<TARGET-PROVIDED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/INTERFACES/CSI_DOOR_SENSOR/LED_STATE</TARGET-PROVIDED-OPERATION-REF>
										</OPERATION-IREF>
									</OPERATION-INVOKED-EVENT>
								</EVENTS>
								
								<RUNNABLES>
									<RUNNABLE-ENTITY>
										<SHORT-NAME>DOOR_READ_OP</SHORT-NAME>
										<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
										<DATA-WRITE-ACCESSS>
											<VARIABLE-ACCESS>
												<SHORT-NAME>SENSOR_VAL</SHORT-NAME>
												<ACCESSED-VARIABLE>
													<AUTOSAR-VARIABLE-IREF>
														<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/PP_SR_DOOR_SATE</PORT-PROTOTYPE-REF>
														<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/INTERFACES/SRI_DOOR_STATE/DOOR_STATE</TARGET-DATA-PROTOTYPE-REF>
													</AUTOSAR-VARIABLE-IREF>		
												</ACCESSED-VARIABLE>
											</VARIABLE-ACCESS>										
									    </DATA-WRITE-ACCESSS>
									    <SYMBOL>DOOR_READ_OP_RUNNABLE</SYMBOL>
									</RUNNABLE-ENTITY>
									
									<RUNNABLE-ENTITY>
										<SHORT-NAME>LED_SWITCH_OP</SHORT-NAME>
											<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
											<SYMBOL>LED_SWITCH_OP_RUNNABLE</SYMBOL>
									</RUNNABLE-ENTITY>
								</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</ECU-ABSTRACTION-SW-COMPONENT-TYPE>
			
			
			<!--============================================================================================-->
			<!--============================================================================================-->
			<!--============================================================================================-->


				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>DOOR_APP_SWC</SHORT-NAME>
						<PORTS>
							<R-PORT-PROTOTYPE>							
								<SHORT-NAME>RP_SR_DOOR_SATE</SHORT-NAME>	
								<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/INTERFACES/SRI_DOOR_STATE/DOOR_STATE</REQUIRED-INTERFACE-TREF>
							</R-PORT-PROTOTYPE>
							<R-PORT-PROTOTYPE>
								<SHORT-NAME>RP_CS_LED_SWITCH</SHORT-NAME>
								<REQUIRED-INTERFACE-TREF DEST="CLIENT-SERVER-INTERFACE">/INTERFACES/CSI_DOOR_SENSOR/DOOR_SENSOR</REQUIRED-INTERFACE-TREF>
							</R-PORT-PROTOTYPE>
						</PORTS>
						
						<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>APP_SWC_RUNNABLES</SHORT-NAME>
							
							<EVENTS>
							<TIMING-EVENT>
								<SHORT-NAME>TE_12ms</SHORT-NAME>
								<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_APP_SWC/DOOR_INDICATION_OP/DOOR_STATE</START-ON-EVENT-REF>
							</TIMING-EVENT>
							</EVENTS>
							
							<RUNNABLES>
							<RUNNABLE-ENTITY>
								<SHORT-NAME>DOOR_INDICATION_OP</SHORT-NAME>
								<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
								<DATA-READ-ACCESSS>
									<VARIABLE-ACCESS>
										<SHORT-NAME>DOOR_STATE</SHORT-NAME>
										<ACCESSED-VARIABLE>
											<AUTOSAR-VARIABLE-IREF>
											<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_APP_SWC/RP_SR_DOOR_SATE</PORT-PROTOTYPE-REF>
												<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">INTERFACES/SRI_DOOR_STATE/DOOR_STATE</TARGET-DATA-PROTOTYPE-REF>
											</AUTOSAR-VARIABLE-IREF>
										</ACCESSED-VARIABLE>
									</VARIABLE-ACCESS>
								</DATA-READ-ACCESSS>
								<SERVER-CALL-POINTS>
									<SYNCHRONOUS-SERVER-CALL-POINT>
										<SHORT-NAME>SERVER_CALL</SHORT-NAME>
										<OPERATION-IREF>
										<CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_APP_SWC/RP_SR_DOOR_SATE</CONTEXT-R-PORT-REF>
										<TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">INTERFACES/CSI_DOOR_SENSOR/DOOR_SENSOR</TARGET-REQUIRED-OPERATION-REF>
										</OPERATION-IREF>
									</SYNCHRONOUS-SERVER-CALL-POINT>
								</SERVER-CALL-POINTS>
							</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
						</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
				
			</ELEMENTS>
		
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>COMPOSITIONS</SHORT-NAME>
			<ELEMENTS>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>TOP_LEVEL_COMPOSITION</SHORT-NAME>
					<COMPONENTS>
					<SW-COMPONENT-PROTOTYPE>
						<SHORT-NAME>ECU_SWC_PROTO</SHORT-NAME>
						<TYPE-TREF DEST="ECU-ABSTRACTION-SW-COMPONENT-TYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC</TYPE-TREF>
					</SW-COMPONENT-PROTOTYPE>
					<SW-COMPONENT-PROTOTYPE>
						<SHORT-NAME>APP_SWC_PROTO</SHORT-NAME>
						<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_APP_SWC</TYPE-TREF>
