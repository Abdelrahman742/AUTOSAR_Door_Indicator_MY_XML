						<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/PP_SR_DOOR_SATE</TARGET-P-PORT-REF>
						</PROVIDER-IREF>
						
						
						<REQUESTER-IREF>
						<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/TOP_LEVEL_COMPOSITION/APP_SWC_PROTO</CONTEXT-COMPONENT-REF>
						<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_APP_SWC/RP_SR_DOOR_SATE</TARGET-R-PORT-REF>
						</REQUESTER-IREF>
					</ASSEMBLY-SW-CONNECTOR>
					
					<ASSEMBLY-SW-CONNECTOR>
						<SHORT-NAME>CS_ASSEMBLY</SHORT-NAME>
						<PROVIDER-IREF>
						<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/TOP_LEVEL_COMPOSITION/ECU_SWC_PROTO</CONTEXT-COMPONENT-REF>
						<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/PP_CS_LED_SWITCH</TARGET-P-PORT-REF>
						</PROVIDER-IREF>
						
						<REQUESTER-IREF>
						<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/TOP_LEVEL_COMPOSITION/APP_SWC_PROTO</CONTEXT-COMPONENT-REF>
						<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/DOOR_INDICATOR_SWCOMPONENTS/DOOR_ECU_SWC/RP_CS_LED_SWITCH</TARGET-R-PORT-REF>
						</REQUESTER-IREF>
					</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			
		
		
					</ELEMENTS>

		</AR-PACKAGE>
	</AR-PACKAGES>
	
	
</AUTOSAR>